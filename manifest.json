{
  "manifest_version": 3,
  "name": "NewTab",
  "version": "1.0.0",
  "description": "A minimal, privacy-first new tab.",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "theme"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "128": "public/icon-128.png"
  },
  "host_permissions": [
    "https://suggestqueries.google.com/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
  
}